{{!--<div class="col-xs-12">--}}
  <div class="table-responsive analysis-table-wrapper ">
    <table class="table table-bordered analysis-table tree-map__table">
      <thead>
      <tr>
        <th class="analysis-table__anchor analysis-table__anchor--white"></th>
        {{#each dates as |date|}}
          <th class="analysis-table__header">
            {{!--todo: set date format based on granularity--}}
            {{moment-format date 'M/D hh:mm a'}}
          </th>
        {{/each}}
      </tr>
      </thead>
      <tbody>
      <tr>
        <td class="analysis-table__anchor analysis-table__anchor--low"></td>
        {{#if rawNumbers}}
          {{#each dates as |date|}}
          <td class="analysis-table__subheader">
            <div class="analysis-table__cell">
              <div class="analysis-table__item">
                Current
              </div>
              <div class="analysis-table__item">
                Baseline
              </div>
              <div class="analysis-table__item analysis-table__item--small">
                Ratio
              </div>
            </div>
          </td>
          {{/each}}
        {{/if}}
      </tr>
      {{#each rows as |metric|}}
      <tr>
        <td class="analysis-table__dimension">
          <div class="analysis-table__cell analysis-table__cell--primary">{{metric.metricName}}</div>
        </td>
        {{!--{{#with metric.subDimensionContributionMap.All.percentageChange as |changes index|}}--}}
          {{#each metric.subDimensionContributionMap.All.percentageChange as |percentChange index|}}
            <td style="background-color: {{compute-color percentChange}}">

              {{!--<div class="analysis-table__item">
                {{lookup metric.subDimensionContributionMap.All.currentValues index}}
              </div>
              <div class="analysis-table__item">
                {{lookup metric.subDimensionContributionMap.All.baselineValues index}}
              </div>--}}
            {{!--<div class="analysis-table__item analysis-table__item--small">
              </div>--}}
              <div class="analysis-table__cell">
                            <div class="analysis-table__item  analysis-table__item--large">

                <span style="color:{{compute-text-color percentChange}}">
                  {{percentChange}}%
                </span>
                </div>
              </div>
            </td>
          {{/each}}
        {{!--{{/with}}--}}
      </tr>
      {{/each}}
     </tbody>
    </table>
  </div>
{{!--</div>--}}
